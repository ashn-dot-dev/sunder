import "sys";

const a: u16 = 0xBEEF;
const b: *u16 = &a;

var c: *u16 = &a;
const d: *any = &a;

func f(x: *u16, b: u32) *u16 {
    *x = (:u16)b;
    return x;
}

func main() void {
    var e: *any = &a;
    var g: func(*any, u32) *any = f;

    var my_x: u16 = 0xF00D;
    g(&my_x, 0xDEADBEEF);

    sys::dump[[bool]]((:usize)b == (:usize)c);
    sys::dump[[bool]]((:usize)b == (:usize)d);
    sys::dump[[bool]]((:usize)b == (:usize)e);
    sys::dump[[u16]](my_x);
}
