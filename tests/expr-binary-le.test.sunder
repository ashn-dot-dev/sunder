import "std";
import "sys";

func foo() ssize {
    return +123s;
}

func bar() ssize {
    return -123s;
}

func main() void {
    const t_le_t: bool = true <= true;
    const f_le_f: bool = false <= false;
    const t_le_f: bool = true <= false;
    const f_le_t: bool = false <= true;
    sys::dump[[bool]](t_le_t);
    sys::dump[[bool]](f_le_f);
    sys::dump[[bool]](t_le_f);
    sys::dump[[bool]](f_le_t);
    sys::dump[[bool]](true <= true);
    sys::dump[[bool]](false <= false);
    sys::dump[[bool]](true <= false);
    sys::dump[[bool]](false <= true);

    std::print(std::err(), "\n");

    const a: ssize = 123s;
    const b: ssize = 456s;
    const c: ssize = a;
    const a_le_a: bool = a <= a;
    const b_le_b: bool = b <= b;
    const a_le_b: bool = a <= b;
    const b_le_a: bool = b <= a;
    const a_le_c: bool = a <= c;
    const b_le_c: bool = b <= c;
    sys::dump[[bool]](a_le_a);
    sys::dump[[bool]](b_le_b);
    sys::dump[[bool]](a_le_b);
    sys::dump[[bool]](b_le_a);
    sys::dump[[bool]](a_le_c);
    sys::dump[[bool]](b_le_c);

    std::print(std::err(), "\n");

    sys::dump[[bool]](3u <= 3u);
    sys::dump[[bool]](3u <= 5u);
    sys::dump[[bool]](5u <= 3u);

    std::print(std::err(), "\n");

    var x: ssize = +1s;
    var y: ssize = -1s;
    sys::dump[[bool]](x <= y);
    sys::dump[[bool]](y <= x);

    std::print(std::err(), "\n");

    sys::dump[[bool]](0x00u8 <= 0xFFu8);
    sys::dump[[bool]](0xFFu8 <= 0x00u8);
    sys::dump[[bool]](0x0000000000000000u64 <= 0xFFFFFFFFFFFFFFFFu64);
    sys::dump[[bool]](0xFFFFFFFFFFFFFFFFu64 <= 0x0000000000000000u64);
}
