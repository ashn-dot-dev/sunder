import "std";
import "sys";

func main() void {
    var vec: std::vector[[u8]] = std::vector[[u8]]::init();
    defer vec.fini();

    sys::dump[[usize]](vec.count());
    sys::dump[[usize]](vec.capacity());

    vec.reserve(0);
    sys::dump[[usize]](vec.count());
    sys::dump[[usize]](vec.capacity());

    vec.reserve(1);
    sys::dump[[usize]](vec.count());
    sys::dump[[usize]](vec.capacity());

    vec.reserve(0);
    sys::dump[[usize]](vec.count());
    sys::dump[[usize]](vec.capacity());

    vec.reserve(1);
    sys::dump[[usize]](vec.count());
    sys::dump[[usize]](vec.capacity());

    vec.reserve(2);
    sys::dump[[usize]](vec.count());
    sys::dump[[usize]](vec.capacity());

    vec.reserve(0xBEEF);
    sys::dump[[usize]](vec.count());
    sys::dump[[usize]](vec.capacity());

    vec.reserve(0);
    sys::dump[[usize]](vec.count());
    sys::dump[[usize]](vec.capacity());
}
