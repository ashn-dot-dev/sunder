import "sys";

struct s {
    var foo: u32;
    var bar: u32;
}

let a: u8 = uninit;
let b: u16 = uninit;
let c: u32 = uninit;
let d: u64 = uninit;
let e: *byte = uninit;
let f: []byte = uninit;
let g: s = uninit;

func main() void {
    sys::dump[[u8]](a);
    sys::dump[[u16]](b);
    sys::dump[[u32]](c);
    sys::dump[[u64]](d);
    sys::dump[[*byte]](e);
    sys::dump[[[]byte]](f);
    sys::dump[[s]](g);

    var a: u8 = uninit;
    var b: u16 = uninit;
    var c: u32 = uninit;
    var d: u64 = uninit;
    var e: *byte = uninit;
    var f: []byte = uninit;
    var g: s = uninit;
    sys::dump[[u8]](a);
    sys::dump[[u16]](b);
    sys::dump[[u32]](c);
    sys::dump[[u64]](d);
    sys::dump[[*byte]](e);
    sys::dump[[[]byte]](f);
    sys::dump[[s]](g);
}
