import "sys/sys.sunder";

const fn_one: func() ssize = one;
const fn_sub: func(ssize, ssize) ssize = sub;
const fn_exit: func(s32) void = sys::exit;

func one() ssize {
    return 1s;
}

func sub(lhs: ssize, rhs: ssize) ssize {
    return lhs - rhs;
}

func main() void {
    fn_exit(
        (:s32)fn_sub(
            fn_sub(
                2s,
                fn_one()),
            fn_one()));
}
