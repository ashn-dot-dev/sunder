# only SUNDER_BACKEND=C
import "std";

func test(str: []byte) void {
    var result = f32::init_from_str(str);
    if result.is_value() {
        var value = result.value();
        std::print_format_line(
            std::out(),
            "{}",
            (:[]std::formatter)[
                std::formatter::init[[f32]](&value)]);
    }
    else {
        std::print_format_line(
            std::out(),
            "{}",
            (:[]std::formatter)[
                std::formatter::init[[[]byte]](&result.error().*.data)]);
    }
}

func main() void {
    test("123");
    test("+123.5");
    test("-123.5");
    test("infinity");
    test("+infinity");
    test("-infinity");
    test("NaN");

    test("inf");
    test("not a number");
    test(" +123.5");
    test(" -123.5");
    test("+123.5 ");
    test("-123.5 ");
}
